{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/consistent-type-definitions": [
      "error",
      "type",
      {
        "allowInterfaces": true
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_", "varsIgnorePattern": "^styles" }
    ]
  }
}
